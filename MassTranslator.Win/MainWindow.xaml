<Window x:Class="MassTranslator.Win.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Mass Translator" Height="350" Width="525">
    <Window.DataContext>
        <MainViewModel
            xmlns="clr-namespace:MassTranslator.Win" />
    </Window.DataContext>
    <Grid>
        <TabControl ItemsSource="{Binding Tabs}">
        </TabControl>
    </Grid>
</Window>
